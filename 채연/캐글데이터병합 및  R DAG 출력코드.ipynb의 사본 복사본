ion']\n","    NUMERICAL2 = ['event_navigate',   'event_note',   'event_object',   'event_person']\n","    ENVIRONMENTAL = ['music', 'fullscreen', 'hq']\n","\n","    dfs = []\n","    grouped = dataset_df.groupby(['session_id', 'level_group'])\n","\n","    # === CATEGORICAL 처리: mode + nunique ===\n","    for col in CATEGORICAL:\n","        # 고유 개수 (행동 다양성)\n","        nunique = grouped[col].nunique()\n","        nunique.name = col + '_nunique'\n","        dfs.append(nunique)\n","\n","    # === ENVIRONMENTAL 처리: 사용자 환경 조작 감지 ===\n","    env_max = grouped[ENVIRONMENTAL].max()\n","    def flag_env(row):\n","        return 1 if not (row['music'] == 1 and row['fullscreen'] == 0 and row['hq'] == 0) else 0\n","\n","    env_flag = env_max.apply(flag_env, axis=1)\n","    env_flag.name = 'env_changed_flag'\n","    dfs.append(env_flag)\n","\n","    # === TEXT 처리: 물음표 및 느낌표로 인한 강조 효과 및 얼마나 다양한 텍스트에 노출되었는가로 ,===\n","    nunique = grouped['text'].nunique()\n","    nunique.name = \"text\" + '_nunique'\n","    dfs.append(nunique)\n","\n","    def count_punctuations(texts):\n","      return texts.fillna(\"\").apply(lambda x: x.count('!') + x.count('?')).sum()\n","\n","    text_punct_cnt = grouped['text'].apply(count_punctuations)\n","    text_punct_cnt.name = 'text_punct_cnt'\n","    dfs.append(text_punct_cnt)\n","\n","    # === NUMERICAL 처리: mean + std ===\n","    for col in NUMERICAL:\n","        mean_val = grouped[col].mean()\n","        mean_val.name = col + '_mean'\n","        dfs.append(mean_val)\n","\n","        std_val = grouped[col].std()\n","        std_val.name = col + '_std'\n","        dfs.append(std_val)\n","\n","    # === NUMERICAL2 처리: mean + std ===\n","    for col in NUMERICAL2:\n","        mean_val = grouped[col].mean()*100\n","        mean_val.name = col + '_mean'\n","        dfs.append(mean_val)\n","\n","        std_val = grouped[col].std()\n","        std_val.name = col + '_std'\n","        dfs.append(std_val)\n","\n","    # === 결합 및 정리 ===\n","    final_df = pd.concat(dfs, axis=1)\n","    final_df = final_df.reset_index()                 # session_id, level_group 포함\n","    final_df = final_df.set_index('session_id')       # session_id 기준 인덱싱\n","\n","    return final_df"],"metadata":{"id":"vwtaExWo0q1g","executionInfo":{"status":"ok","timestamp":1767091910572,"user_tz":-540,"elapsed":32,"user":{"displayName":"‎장예지(자연과학대학 통계학과)","userId":"11787690279681006560"}}},"execution_count":46,"outputs":[]},{"cell_type":"code","source":["from sklearn.preprocessing import LabelEncoder\n","import pandas as pd\n","\n","def final_level_encoding_dag(df, min_bins=2, max_bins=5):\n","    \"\"\"\n","    DAG 입력용 최종 전처리:\n","    - *_mode 로 끝나는 컬럼 → Label Encoding \\\n","    - *_mean, *_std, *_nunique 로 끝나는 컬럼 → qcut 이산화 (수치형 레벨화)\n","    \"\"\"\n","\n","    df_encoded = df.copy()\n","\n","    # 1. 범주형 처리: *_mode 컬럼만\n","    cat_cols = [col for col in df_encoded.columns if col.endswith('_mode')]\n","    for col in cat_cols:\n","        le = LabelEncoder()\n","        df_encoded[col] = le.fit_transform(df_encoded[col].astype(str))\n","\n","    # 2. 수치형 처리: *_mean, *_std, *_nunique\n","    num_cols = [col for col in df_encoded.columns if col.endswith(('_mean', '_std', '_nunique'))]\n","    for col in num_cols:\n","        try:\n","            nunique = df_encoded[col].nunique()\n","            bins = min(max_bins, max(min_bins, min(nunique, max_bins)))  # 2~4 사이\n","            df_encoded[col] = pd.qcut(df_encoded[col], q=bins, labels=False, duplicates='drop')\n","        except Exception:\n","            df_encoded[col] = -1  # qcut 실패 시 fallback\n","\n","    return df_encoded\n","\n","\n"],"metadata":{"id":"T52sMuJdoL56","executionInfo":{"status":"ok","timestamp":1767089376531,"user_tz":-540,"elapsed":244,"user":{"displayName":"‎장예지(자연과학대학 통계학과)","userId":"11787690279681006560"}}},"execution_count":27,"outputs":[]},{"cell_type":"code","source":["df_dag_ready = final_level_encoding_dag(dataset_df)"],"metadata":{"id":"LdqpUF4nRbKk"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["def print_level_distribution_all_columns(df):\n","    \"\"\"\n","    모든 컬럼에 대해 level별 빈도수 및 비율(%) 출력\n","    \"\"\"\n","    for col in df.columns:\n","        print(f\"\\n🔹 [{col}] 레벨 분포:\")\n","        counts = df[col].value_counts(dropna=False).sort_index()\n","        total = counts.sum()\n","        for level, count in counts.items():\n","            pct = count / total * 100\n","            print(f\"  level {level}: {count:,} ({pct:.1f}%)\")\n","\n","print_level_distribution_all_columns(df_dag_ready)"],"metadata":{"id":"uciFZp5hRblX"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# 위에서 각 칼럼별로 레벨별 분포 찍어보니까 데이터 불균형으로 인해 level 이 한개의 그룹으로만 쪼개진 얘들\n","## - 바로 환경변수!(음악, 고화길, 풀화면) 는 걍 버립니다.. 어케 처리할지 고민이 필요합니다.\n","cols_to_drop = [\n","    'music_mean', 'music_std',\n","    'fullscreen_mean', 'fullscreen_std',\n","    'hq_mean', 'hq_std',\n","    'name_mode'\n","]\n","\n","# 실제 드롭\n","df_dag_ready = df_dag_ready.drop(columns=cols_to_drop, errors='ignore')"],"metadata":{"id":"xjhgvqaXRgf0"},"execution_count":null,"outputs":[]},{"cell_type":"markdown","source":["### train 과 label 최종 병합"],"metadata":{"id":"1QQBDMwESLhn"}},{"cell_type":"code","source":["df_dag_ready = df_dag_ready.reset_index()\n","# 데이터 타입 맞추기\n","df_dag_ready['session_id'] = df_dag_ready['session_id'].astype(str)\n","correct_mean['session_id'] = correct_mean['session_id'].astype(str)\n","\n","df_merged = df_dag_ready.reset_index().merge(correct_mean, on=['session_id', 'level_group'], how='left')\n","df_merged = df_merged.drop(columns=['index'], errors='ignore')"],"metadata":{"id":"p4whrgNgRwW1"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["df_merged.columns\n","# 잘병합됐는지 확인~"],"metadata":{"id":"7O4sa--cRyhE"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["df_final = df_merged.copy()\n","df_final.to_csv(\"df_final.csv\", index=False)\n","\n","# r에서 베이지안 돌리려고 아웃풋으로 빼내서 r 에서 이후 실행함."],"metadata":{"id":"rSBCQhaRR-Ip"},"execution_count":null,"outputs":[]},{"cell_type":"markdown","source":["### R에서 진행한 DAG 그래프 출력 코드"],"metadata":{"id":"vXNdljLNUbVx"}},{"cell_type":"markdown","source":["\n","\n","```\n","library(bnlearn)\n","library(dplyr)\n","library(visNetwork)\n","library(bnviewer)\n","library(ggplot2)\n","\n","# DAG 이쁘게 그려줄 함수 명시\n","\n","plot_network = function(dag, strength_df = NULL, undirected = FALSE,\n","                        group = NA, title = NULL, height = NULL, width = NULL) {\n","  \n","  # 엣지 굵기 지정 (strength_df가 주어졌다면 그걸 기준으로)\n","  edge_size = ifelse(\n","    is.null(strength_df),\n","    NA,\n","    right_join(strength_df, data.frame(dag$arcs[, c(1, 2)]))$strength\n","  )\n","  \n","  # 노드 설정\n","  nodes = names(dag$nodes)\n","  nodes = data.frame(\n","    id = nodes,\n","    label = nodes,\n","    size = 16,\n","    font.size = 18,\n","    shadow = TRUE,\n","    group = group\n","  )\n","  \n","  # 엣지 설정\n","  edges = data.frame(\n","    from = dag$arcs[, 1],\n","    to = dag$arcs[, 2],\n","    value = edge_size,\n","    arrows = list(to = list(enabled = TRUE, scaleFactor = 0.5)),\n","    shadow = TRUE\n","  )\n","  \n","  # group 또는 edge_size가 NA인 경우 제거\n","  if (is.na(group[1])) nodes = nodes[, -6]\n","  if (is.na(edge_size[1])) edges = edges[, -3]\n","  if (undirected) edges$arrows.to.enabled = FALSE\n","  \n","  # visNetwork로 그리기\n","  network = visNetwork(nodes, edges, main = title, height = height, width = width) %>%\n","    visOptions(highlightNearest = TRUE, nodesIdSelection = TRUE)\n","  \n","  return(network)\n","}\n","\n","# 1. 데이터 로드(파이썬에서 전처리 진행)\n","df <- read.csv(\"df_final.csv\") %>% mutate_if(is.integer, as.factor)\n","\n","# 2. 0-4 레벨 그룹만 추출 -> 다른 그룹에 대하여 코드 재사용하여 반복 진행\n","df_04 <- subset(df, level_group == \"0-4\")\n","\n","# 3. 해석의 용이성 및 이후 조건부 분포 시각화를 위해 종속변수를 범주형으로 변환함. 기준값은 density plot 참고하여 수정으로 지정\n","\n","ggplot(df_04, aes(x = correct_mean)) +\n","  geom_density(fill = \"lightblue\", alpha = 0.6) +\n","  labs(title = \"Density of correct_mean\",\n","       x = \"correct_mean\",\n","       y = \"Density\") +\n","  theme_minimal()\n","\n","# 직접 범위 기준으로 자르기 (예: 0.5 기준으로 high/low, 또는 3구간)\n","df_04$correct_level <- cut(df_04$correct_mean,\n","                           breaks = c(-Inf, 0.4, 0.8, Inf),\n","                           labels = c(\"low\", \"mid\", \"high\"),\n","                           right = TRUE)\n","\n","df_04$correct_level <- factor(df_04$correct_level)\n","\n","# 4. session_id, level_group 제거 (네트워크에 필요 없음)\n","\n","df_04 <- df_04[, !(names(df_04) %in% c(\"session_id\", \"level_group\",\"correct_mean\"))]\n","\n","# 블랙리스트: correct_level → 다른 변수는 금지\n","bl_04 <- data.frame(\n","  from = \"correct_level\",\n","  to = setdiff(names(df_04), \"correct_level\")\n",")\n","# DAG 학습\n","bn_04 <- hc(df_04, blacklist = bl_04)\n","\n","# DAG 그래프 만들기\n","plot_network(bn_04, group = group, title = \"Hill-Climbing DAG - Level Group 0-4, class target\")\n","\n","# 부모노드 찍기\n","parents_vars <- parents(bn_04, \"correct_level\")\n","print(parents_vars)\n","\n","# 부모노드(2개)에 대하여 결합분포 찍기\n","\n","# 부모노드만 필터링\n","dag_subset_vars <- c(parents_vars, \"correct_level\")  \n","df_subset <- df_04[, dag_subset_vars]\n","\n","# 조건부 확률 추정 (CPT 추정)\n","\n","dag_small <- hc(df_subset)\n","dag_fit_small <- bn.fit(dag_small, df_subset)\n","\n","# 조건부 확률 시각화 (dotplot)\n","bn.fit.dotplot(dag_fit_small$correct_level)  \n","\n","```\n","\n"],"metadata":{"id":"oZe2OXvWTM8f"}}]}